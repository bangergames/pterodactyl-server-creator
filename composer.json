{
    "name": "bangergames/pterodactyl-server-creator",
    "description": "Pterodactyl server creator for Laravel",
    "homepage": "https://github.com/bangergames/pterodactyl-server-creator",
    "license": "MIT",
    "authors": [
        {
            "name": "Kenan Duman",
            "email": "kenanduman1988@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^6.2 || ^7.0",
        "illuminate/support": "~5.6 || ~6.0 || ~7.0 || ~8.0",
        "illuminate/console": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "BangerGames\\PterodactylServerCreator\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BangerGames\\PterodactylServerCreator\\PterodactylServerCreatorProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
